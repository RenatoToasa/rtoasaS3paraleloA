<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="rtoasaS3.Page1">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stackLayoutStyle}">
            
            <Label Text="SEMAMA 3: ESTILO 1" FontSize="18"  HorizontalTextAlignment="Center"/>
            <Entry x:Name="txtDatoUno" Placeholder="Ingrese una cadena"/>
            <Entry x:Name="txtDatoDos" Placeholder="Ingrese un numero" Keyboard="Numeric"/>
            <Button x:Name="btnAbrir" Text="ABRIR" Clicked="btnAbrir_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>